{
  "name": "rewinds",
  "description": "Web app starter kit with Remix and Tailwind CSS",
  "version": "0.0.0",
  "license": "MIT",
  "author": "M Haidar Hanif <git@mhaidarhanif.com> (https://mhaidarhanif.com)",
